extends layout

block content
  h1 Patrons
  if errors
    include partials/val_errors
  p
    a.button(href="/library/patrons/new") Create New Patron
  table
    thead
      tr
        th Name
        th Address
        th Email
        th Library ID
        th Zip
    if patrons
      each patron in patrons
        include partials/patron
